{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "serverless/schedule/generate.py",
      "use": "@vercel/python",
      "config": {
        "includeFiles": ["serverless/_python/**"]
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/schedule/generate",
      "destination": "/serverless/schedule/generate"
    }
  ]
}
